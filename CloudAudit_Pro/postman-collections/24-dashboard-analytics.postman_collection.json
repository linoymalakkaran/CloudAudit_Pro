        "url": {"raw": "{{base_url}}/dashboard/overview", "host": ["{{base_url}}"], "path": ["dashboard", "overview"]}
      },
      "response": []
    },
    {
      "name": "Get Audit Progress",
      "request": {
        "method": "GET",
        "header": [{"key": "Authorization", "value": "Bearer {{auth_token}}"}],
        "url": {"raw": "{{base_url}}/dashboard/audit-progress/{{period_id}}", "host": ["{{base_url}}"], "path": ["dashboard", "audit-progress", "{{period_id}}"]}
      },
      "response": []
