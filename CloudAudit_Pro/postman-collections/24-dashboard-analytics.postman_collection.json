      "request": {
        "method": "GET",
        "header": [{"key": "Authorization", "value": "Bearer {{auth_token}}"}],
        "url": {"raw": "{{base_url}}/analytics/period/{{period_id}}", "host": ["{{base_url}}"], "path": ["analytics", "period", "{{period_id}}"]}
      },
      "response": []
    },
    {
      "name": "Get Team Performance",
      "request": {
        "method": "GET",
        "header": [{"key": "Authorization", "value": "Bearer {{auth_token}}"}],
        "url": {"raw": "{{base_url}}/analytics/team-performance", "host": ["{{base_url}}"], "path": ["analytics", "team-performance"]}
