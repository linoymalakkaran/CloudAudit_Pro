{
  "info": {
    "name": "27 - Data Management",
    "_postman_id": "data-management-collection-027",
    "description": "Data import and export endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Import Data (CSV)",
      "request": {
        "method": "POST",
        "header": [{"key": "Authorization", "value": "Bearer {{auth_token}}"}],
        "body": {
          "mode": "formdata",
          "formdata": [
            {"key": "file", "type": "file", "src": []},
            {"key": "dataType", "value": "TRIAL_BALANCE", "type": "text"},
            {"key": "periodId", "value": "{{period_id}}", "type": "text"}
          ]
        },
        "url": {"raw": "{{base_url}}/data/import", "host": ["{{base_url}}"], "path": ["data", "import"]}
      },
      "response": []
    },
    {
      "name": "Export Data (Excel)",
      "request": {
        "method": "POST",
        "header": [
          {"key": "Content-Type", "value": "application/json"},
          {"key": "Authorization", "value": "Bearer {{auth_token}}"}
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"dataType\": \"TRIAL_BALANCE\",\n  \"periodId\": \"{{period_id}}\",\n  \"format\": \"EXCEL\"\n}"
        },
        "url": {"raw": "{{base_url}}/data/export", "host": ["{{base_url}}"], "path": ["data", "export"]}
      },
      "response": []
    },
    {
      "name": "Get Import History",
      "request": {
        "method": "GET",
        "header": [{"key": "Authorization", "value": "Bearer {{auth_token}}"}],
        "url": {"raw": "{{base_url}}/data/import/history", "host": ["{{base_url}}"], "path": ["data", "import", "history"]}
      },
      "response": []
    }
  ],
  "auth": {"type": "bearer", "bearer": [{"key": "token", "value": "{{auth_token}}", "type": "string"}]}
}
